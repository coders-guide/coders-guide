{
  "name": "coders-guide-app",
  "version": "0.4.0",
  "main": "src/index.tsx",
  "scripts": {
    "start": "webpack-dev-server --content-base=./dist/ --host 0.0.0.0",
    "build:staging": "cross-env TARGET_ENV=staging webpack --mode=production",
    "build:production": "cross-env TARGET_ENV=production webpack --mode=production",
    "deploy:s3:staging": "aws s3 sync dist $CG_DEPLOY_STAGING --delete",
    "deploy:s3:production": "aws s3 sync dist $CG_DEPLOY_PRODUCTION",
    "invalidate:staging": "aws cloudfront create-invalidation --distribution-id $CG_DISTRIBUTION_STAGING --paths '/*'",
    "invalidate:production": "aws cloudfront create-invalidation --distribution-id $CG_DISTRIBUTION_PRODUCTION --paths '/*'",
    "deploy:staging": "npm run deploy:s3:staging && npm run invalidate:staging",
    "deploy:production": "npm run deploy:s3:production && npm run invalidate:production"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/jest": "^24.0.18",
    "@types/uuid": "^3.4.4",
    "awesome-typescript-loader": "^5.2.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "@types/fontfaceobserver": "^0.0.6",
    "@types/intro.js": "^2.4.6",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.1",
    "csv": "^5.3.1",
    "es6-promise": "^4.2.8",
    "file-loader": "^5.0.2",
    "fontfaceobserver": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "immutable": "^4.0.0-rc.12",
    "intro.js": "^2.9.3",
    "mini-css-extract-plugin": "^0.9.0",
    "react": "^16.12.0",
    "react-archer": "^1.5.0",
    "react-dom": "^16.12.0",
    "react-gtm-module": "^2.0.8",
    "sass": "^1.24.2",
    "sass-loader": "^8.0.0",
    "simple-scrollbar": "^0.4.0",
    "style-loader": "^1.1.2",
    "ts-loader": "^6.2.1",
    "typescript-loader": "^1.1.3",
    "uuid": "^3.3.2",
    "webpack-cleanup-plugin": "^0.5.1"
  }
}
